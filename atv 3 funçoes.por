programa {
  funcao inicio() {
    real n1, n2
    escreva("Digite o primeiro numero: ")
    leia(n1)
    escreva("Digite o segundo numero: ")
    leia(n2)

    se(n1>n2)
    escreva("O primeiro numero é maior que o segundo ")
    senao 
    se(n2>n1)
      escreva("O segundo numero é maior que o primeiro")
    
  }
}
